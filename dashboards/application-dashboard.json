{
  "dashboard": {
    "id": null,
    "title": "Applications Monitoring Dashboard",
    "tags": ["applications", "n8n", "postgres", "redis"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "n8n Application Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"n8n\"}",
            "legendFormat": "n8n Workflows"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        },
        "gridPos": {"h": 6, "w": 4, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "PostgreSQL Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"postgres_exporter\"}",
            "legendFormat": "PostgreSQL"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        },
        "gridPos": {"h": 6, "w": 4, "x": 4, "y": 0}
      },
      {
        "id": 3,
        "title": "Redis Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"redis_exporter\"}",
            "legendFormat": "Redis Cache"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        },
        "gridPos": {"h": 6, "w": 4, "x": 8, "y": 0}
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{datname=\"n8n\"}",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 5,
        "title": "Redis Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Memory Used"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 6,
        "title": "Application Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
            "legendFormat": "Avg Response Time"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 14}
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}